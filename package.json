{
  "name": "rhythmic-realm",
  "version": "0.2.0",
  "repository": "rhythmic-realm",
  "description": "Three.js biolerplate with ES6, Webpack and React/Redux controls",
  "keywords": [
    "react",
    "redux",
    "webgl",
    "boilerplate",
    "three.js",
    "es6",
    "webpack",
    "CPSC 478",
    "spotify api",
    "keyword2",
    "...",
    "this last entry CANNOT have a comma after it ->"
  ],
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack",
    "clean": "rimraf ./build && mkdir ./build",
    "lint": "eslint ./app/**/*.js",
    "reactman": "reactman --config ./templates/reactman.config.js",
    "deploy": "npm run prebuild && webpack --mode production --progress && gh-pages -d build"
  },
  "config": {
    "title": "Rhythmic Realm",
    "build": "./build/"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^20.0.3",
    "gsap": "^1.19.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "three": "^0.87.1",
    "three-effectcomposer-es6": "0.0.4",
    "three-obj-loader": "^1.1.3",
    "three-orbit-controls": "^82.1.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.6.1",
    "babel-loader": "^8.1.0",
    "eslint": "^4.10.0",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.4",
    "raw-loader": "^0.5.1",
    "reactman": "^3.5.2",
    "rimraf": "^2.6.0",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.10.3",
    "cookie-parser": "1.3.2",
    "express": "~4.16.0",
    "cors": "^2.8.4",
    "querystring": "~0.2.0",
    "request": "~2.83.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
